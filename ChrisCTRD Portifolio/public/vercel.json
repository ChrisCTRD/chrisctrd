{
  "rewrites": [
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/commissions",
      "destination": "/commissions.html"
    },
    {
      "source": "/portfolio",
      "destination": "/portfolio.html"
    },
    {
      "source": "/tos",
      "destination": "/tos.html"
    },
    {
      "source": "/(.*)",
      "status": 404,
      "destination": "/404.html"
    }
  ],
  "cleanUrls": true
}
